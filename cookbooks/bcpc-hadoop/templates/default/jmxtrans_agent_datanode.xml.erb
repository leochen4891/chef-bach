<jmxtrans-agent>
  <queries>
    <% (@queries || {}).each do |query| %>
      <query objectName="<%= query['objectName'].to_s %>" attributes="<%= query['attributes'] %>" resultAlias="<%= query['resultAlias'] %>.#attribute#"/>
    <% end %>
  </queries>

  <outputWriter class="<%= @outputWriter_class %>">
      <host><%= @outputWriter_host %></host>
      <port><%= @outputWriter_port %></port>
      <namePrefix><%= @outputWriter_namePrefix %>.#hostname#</namePrefix>
  </outputWriter>

  <collectIntervalInSeconds><%= @collectIntervalInSeconds %></collectIntervalInSeconds>
</jmxtrans-agent>
