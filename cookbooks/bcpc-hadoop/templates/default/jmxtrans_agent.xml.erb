<jmxtrans-agent>
  <queries>
    <% (@queries || {}).each do |query| %>
      <query objectName="<%= query['objectName'] %>" attributes="<%= query['attributes'] %>" resultAlias="<%= query['resultAlias'] %>"/>
    <% end %>
  </queries>

  <outputWriter class="<%= @output_writer_class %>">
      <host><%= @output_writer_host %></host>
      <port><%= @output_writer_port %></port>
      <namePrefix><%= @output_writer_name_prefix %>.#escaped_hostname#.</namePrefix>
  </outputWriter>

  <collectIntervalInSeconds><%= @collect_interval_in_seconds %></collectIntervalInSeconds>
</jmxtrans-agent>
